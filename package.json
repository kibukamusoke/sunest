{
  "name": "sunest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "debug:env": "node scripts/debug-env.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "openapi:angular": "rimraf ./sdk/angular && npx @openapitools/openapi-generator-cli generate -i ./openapi/swagger.json -g typescript-angular -o ./sdk/angular -c ./openapi/openapi-generator-config.json",
    "openapi:react": "rimraf ./sdk/react && npx openapi-typescript-codegen --input ./openapi/swagger.json --output ./sdk/react --client axios --name SunestApi",
    "openapi:typescript": "rimraf ./sdk/typescript && npx swagger-typescript-api generate -p ./openapi/swagger.json -o ./sdk/typescript -n api.ts --axios",
    "openapi:flutter": "rimraf ./sdk/flutter && mkdir -p ./sdk/flutter && npx @openapitools/openapi-generator-cli generate -i ./openapi/swagger.json -g dart-dio -o ./sdk/flutter",
    "generate:all-sdks": "npm run openapi:angular && npm run openapi:react && npm run openapi:typescript && npm run openapi:flutter"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.782.0",
    "@aws-sdk/s3-request-presigner": "^3.782.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.14",
    "@nestjs/platform-socket.io": "^11.0.14",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.1.1",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.0.14",
    "@prisma/client": "^6.6.0",
    "@types/bcrypt": "^5.0.2",
    "@types/multer": "^1.4.12",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "firebase-admin": "^13.2.0",
    "form-data": "^4.0.2",
    "hbs": "^4.2.0",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "mailgun.js": "^12.0.1",
    "multer": "^1.4.5-lts.2",
    "multer-s3": "^3.0.1",
    "nestjs-pino": "^4.4.0",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.66.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.14.1",
    "pino-http": "^10.4.0",
    "prisma": "^6.6.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@openapitools/openapi-generator-cli": "^2.18.4",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.64.8",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "openapi-typescript-codegen": "^0.29.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swagger-typescript-api": "^13.0.28",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
